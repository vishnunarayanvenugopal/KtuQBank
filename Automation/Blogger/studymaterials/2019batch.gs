       else if(fullrowsstudymaterials[pdfrow[y]][10]=="4")
      {
        mod4.push(y);
      }
       else if(fullrowsstudymaterials[pdfrow[y]][10]=="5")
      {
        mod5.push(y);
      }
       else if(fullrowsstudymaterials[pdfrow[y]][10]=="6")
      {
        mod6.push(y);
      }
    }
  mod.push(mod1,mod2,mod3,mod4,mod5,mod6);
  return mod;
}
